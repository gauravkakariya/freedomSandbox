        /****************************************************/
        for(Integer iCount = 0;iCount < goalFrequency;iCount++)
        {
          dblRemainingAmount = dblRemainingAmount + (dblRemainingAmount * (dblGrowthrateInRemainingAmount / 100));
        }  
        return dblRemainingAmount;
    }
       
