		{			
			if(objAccount.OPS_Team_Member__c == null)
        	{
 				ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.INFO ,'Please add Operations Team Member(MST2) for Client "'+objAccount.Name+'".'));
        		return null;
 			}
		}
		displayPopup = true;   			       	
	   	Account objBPAccount = new Account();
		Contact objContact = new Contact();
		Messaging.SingleEmailMessage emailToOperations = new Messaging.SingleEmailMessage();
		List<String> lstToOperationAddresses = new List<String>();
		List<Event_Master__c> lstEM = [Select e.Task_Event_Master__c, e.Name, e.Event_Id__c, e.Escalation_Event_Master__c
        									From Event_Master__c e
        									where isActive__c = true 
