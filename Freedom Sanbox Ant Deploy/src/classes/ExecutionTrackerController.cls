		}
		Account objAccount = [Select Id, Name,Related_To__c ,OwnerId,owner.name,OPS_Team_Member__r.Team_Member__c,
								Virtual_Partner__c,Business_Unit__c  
								from Account where Id =: beanObj.entityId];
