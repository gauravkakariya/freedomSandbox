			if(objET.Application_Status__c == '' || objET.Application_Status__c == null || (objET.Application_Status__c == Label.Forms_Handed && objET.ET_Status__c == 'Assigned to MET'))
			{
				system.debug('***objET*******'+objET);
				lstAssignToOpsETRecords.add(objET);
