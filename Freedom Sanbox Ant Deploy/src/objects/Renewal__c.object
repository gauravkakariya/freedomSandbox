    <validationRules>
        <fullName>Cheque_Received_Date</fullName>
        <active>true</active>
        <description>Cheque Received Date cannot be null</description>
        <errorConditionFormula>OR(
AND(ISNULL(Cheque_Received_Date__c),ISPICKVAL(Has_Opted_for_Renewal__c,&apos;Yes&apos;)),
AND(NOT(ISNULL(Cheque_Received_Date__c)),NOT(ISPICKVAL(Has_Opted_for_Renewal__c,&apos;Yes&apos;)))
)</errorConditionFormula>
        <errorMessage>Cheque Received Date has to be entered if &apos;Has Opted for Renewal&apos; equals to &apos;Yes&apos; and vice versa</errorMessage>
    </validationRules>
