    <fields>
        <fullName>Client__c</fullName>
        <description>Master detail relationship field that relates Client Interests to Clients (Account)</description>
        <externalId>false</externalId>
        <label>Client</label>
        <referenceTo>Account</referenceTo>
        <relationshipName>Client_Interests</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Interest_Type__c</fullName>
        <description>Is the client interested in buying or selling this security?</description>
        <externalId>false</externalId>
        <label>Interest Type</label>
        <picklist>
            <picklistValues>
                <fullName>Buy</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Sell</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>On_Watch_List__c</fullName>
